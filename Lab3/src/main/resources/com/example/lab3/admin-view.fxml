<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.embed.swing.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<AnchorPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/17.0.12" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.lab3.AdminController">

    <Label layoutX="20.0" layoutY="15.0" style="-fx-font-size: 20px;" text="Chose shelter: " />
    <SwingNode fx:id="animalsNode" AnchorPane.bottomAnchor="50.0" AnchorPane.leftAnchor="10.0" AnchorPane.rightAnchor="10.0" AnchorPane.topAnchor="50.0" />
    <SwingNode fx:id="shelterSelectionNode" layoutX="150.0" layoutY="18.0" />

    <Button fx:id="addButton" layoutX="10.0" AnchorPane.bottomAnchor="15.0" prefWidth="80.0" text="Add" onAction="#handleAdd" />
    <Button fx:id="editButton" layoutX="100.0" AnchorPane.bottomAnchor="15.0" prefWidth="80.0" text="Edit" onAction="#handleEdit" />
    <Button fx:id="deleteButton" layoutX="190.0" AnchorPane.bottomAnchor="15.0" prefWidth="80.0" text="Delete" onAction="#handleDelete" />



</AnchorPane>
